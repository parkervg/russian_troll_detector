    "def stops_group(x):\n",
    "    if x > -2:\n",
    "        return(0)\n",
    "    if x < -1.1 and x > -4:\n",
    "        return(1)\n",
    "    if x < -4:\n",
    "        return(2)\n",
    "df[\"suspicion\"] = [stops_group(x) for x in df.stop_score]"
